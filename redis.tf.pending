module "redis" {
  source = "modules/redis"

  StackName           = "${var.StackName}"
  SubnetID            = "${module.vpc.elasticache_subnets}"
  WebappSecurityGroup = "${module.application.WebappSecurityGroup}"
  InstanceType        = "${var.RedisInstanceType}"
  vpc_id              = "${module.hyrax_vpc.vpc_id}"
}
